{
    "name": "phpvms/phpvms",
    "description": "phpVMS - Virtual Airline Administration",
    "keywords": ["phpvms", "virtual", "airlines"],
    "license": "BSD-3-Clause",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "http://www.phpvms.net",
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/arthurpar06/laravel-theme"
        },
        {
          "type": "vcs",
          "url": "https://github.com/arthurpar06/filament-modules"
        }
      ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-bcmath": "*",
        "ext-pdo": "*",
        "ext-intl": "*",
        "ext-zip": "*",
        "fisharebest/ext-calendar": "^2.5",
        "symfony/flex": "^2.4.1",
        "symfony/polyfill-iconv": "~1.22.0",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php82": "*",
        "symfony/deprecation-contracts": "~v3.2.0",
        "symfony/event-dispatcher-contracts": "^3.5.0",
        "symfony/mailgun-mailer": "^7.2",
        "symfony/console": "^7.2",
        "symfony/http-client": "^7.2",
        "symfony/yaml": "^7.2",
        "psr/container": "1.1.1",
        "composer/composer": "^2.7.7",
        "composer/installers": "^2.3.0",
        "laravel/framework": "^v11.38",
        "arrilot/laravel-widgets": "~3.13.0",
        "guzzlehttp/guzzle": "^7.9.0",
        "hashids/hashids": "~4.1.0",
        "igaster/laravel-theme": "dev-master",
        "intervention/image": "~2.4",
        "jmikola/geojson": "^1.2.0",
        "joshbrw/laravel-module-installer": "~2.0.1",
        "laracasts/flash": "~3.2.3",
        "laravel/helpers": "^v1.7.1",
        "jeremykendall/php-domain-parser": "~6.3",
        "league/commonmark": "^2.6",
        "league/csv": "^9.14.0",
        "league/geotools": "~1.2.0",
        "league/iso3166": "~4.2.0",
        "markrogoyski/math-php": "~2.5.0",
        "myclabs/deep-copy": "~1.10.0",
        "nabeel/vacentral": "~2.1.0",
        "nwidart/laravel-modules": "~11.0.0",
        "php-units-of-measure/php-units-of-measure": "~2.1.0",
        "prettus/l5-repository": "~2.9.1",
        "spatie/valuestore": "~1.3.2",
        "tivie/php-os-detector": "~1.1.0",
        "vlucas/phpdotenv": "^5.6",
        "webpatser/laravel-uuid": "~4.0.1",
        "oomphinc/composer-installers-extender": "~2.0.0",
        "laravel/ui": "^4.2.2",
        "madnest/madzipper": "^1.5.0",
        "elcobvg/laravel-opcache": "^0.4.1",
        "fakerphp/faker": "^v1.19.0",
        "jpkleemans/attribute-events": "^1.4.0",
        "akaunting/laravel-money": "^6.0",
        "staudenmeir/belongs-to-through": "^v2.16.0",
        "staudenmeir/eloquent-has-many-deep": "1.20.0",
        "spatie/laravel-ignition": "^2.9",
        "kyslik/column-sortable": "^6.6",
        "jlorente/laravel-data-migrations": "^2.0",
        "filament/filament": "^3.2.133",
        "flowframe/laravel-trend": "^0.2.0",
        "bezhansalleh/filament-shield": "^3.2.4",
        "guava/filament-clusters": "^1.2.0",
        "spatie/laravel-backup": "^9.2",
        "shuvroroy/filament-spatie-laravel-backup": "^2.2.3",
        "laravel/socialite": "^5.16",
        "socialiteproviders/discord": "^4.2",
        "symfony/postmark-mailer": "^7.2",
        "league/html-to-markdown": "^5.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "nikolaposa/version": "^4.2",
        "spatie/laravel-activitylog": "^4.9.1",
        "socialiteproviders/vatsim": "^5.0",
        "socialiteproviders/ivao": "^4.0",
        "mailersend/laravel-driver": "^2.6",
        "arxeiss/sansdaemon": "^1.3",
        "coolsam/modules": "3.x-dev",
        "opcodesio/log-viewer": "^3.14"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14.10",
        "filp/whoops": "^2.16",
        "friendsofphp/php-cs-fixer": "^3.56.0",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^v8.1.0",
        "phpunit/phpunit": "^10.5",
        "laravel/sail": "^1.39",
        "laravel/pint": "^1.19",
        "kitloong/laravel-migrations-generator": "^7.0",
        "rector/rector": "^2.0",
        "driftingly/rector-laravel": "^2.0",
        "laravel/pail": "^1.2"
    },
    "autoload": {
        "classmap": [
            "app/Database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
     "extra": {
         "installer-types": ["phpvms-module"],
         "installer-paths": {
             "modules/{$name}/": ["type:phpvms-module"]
         },
         "module-dir": "modules",
         "symfony": {
            "require": "^7.0"
        }
    },
    "scripts": {
        "pre-package-uninstall": [
            "@php -r \"array_map('unlink', glob('bootstrap/cache/*'));\""
        ],
        "post-package-uninstall": [
            "@php artisan package:discover --ansi",
            "@php artisan phpvms:caches"
        ],
        "pre-autoload-dump": [
            "@php -r \"array_map('unlink', glob('bootstrap/cache/*'));\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan phpvms:caches",
            "@php artisan filament:upgrade"
        ],
        "pint": "pint --dirty",
        "test": "phpunit",
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "config": {
        "bin-dir": "vendor/bin/",
        "allow-plugins": {
            "composer/installers": true,
            "joshbrw/laravel-module-installer": true,
            "oomphinc/composer-installers-extender": true,
            "symfony/flex": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "wikimedia/composer-merge-plugin": true
        }
    }
}
